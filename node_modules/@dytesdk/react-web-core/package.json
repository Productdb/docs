{
  "name": "@dytesdk/react-web-core",
  "description": "React wrapper for @dytesdk/web-core.",
  "license": "UNLICENSED",
  "version": "1.36.4-stripped.1",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./types/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-in/react-web-core.git"
  },
  "bugs": {
    "url": "https://community.dyte.io"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "is-ci || husky install",
    "lint": "eslint . --ext .ts --ext .js --ext .jsx --ext .tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests",
    "prepublishOnly": "cp package.json package.json.bak && node prepublish.js",
    "postpublish": "mv package.json.bak package.json"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "@dytesdk/web-core": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.0.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.0",
    "is-ci": "^3.0.1",
    "jest": "^28.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
