{
  "name": "@dytesdk/web-core",
  "version": "1.31.0-stripped.1",
  "description": "A SDK that provides Dyte's core functionality.",
  "author": "https://github.com/dyte-in",
  "license": "UNLICENSED",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs.js",
  "browser": "./dist/index.iife.js",
  "module": "./dist/index.es.js",
  "react-native": "./dist/index.rn.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./inlined": {
      "default": "./dist/index.iife.js",
      "types": "./dist/index.d.ts"
    },
    "./compatible": {
      "import": "./dist/index.es5.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-in/web-core.git"
  },
  "bugs": {
    "url": "https://community.dyte.io"
  },
  "homepage": "https://dyte.io",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "build:es:cjs": "vite build -c ./.config/vite.config.ts",
    "build:iife": "VITE_IIFE=true vite build -c ./.config/vite.iife.config.ts",
    "build:rn": "vite build -c ./.config/vite.rn.config.ts",
    "build:types": "./.config/build-types.sh",
    "build": "concurrently --names \"ES-CJS,IIFE,RN,TYPES\" \"npm run build:es:cjs\" \"npm run build:iife\" \"npm run build:rn\" \"npm run build:es5\" && npm run build:types",
    "build:demo": "vite build -c ./.config/vite.demo.config.ts",
    "build:watch": "VITE_IIFE=true vite build -c ./.config/vite.iife.config.ts --watch",
    "build:es5": "vite build -c ./.config/vite.es5.config.ts",
    "dev": "vite",
    "dev:coverage": "vite -c ./.config/vite.coverage.config.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "analyze-bundle": "VITE_ANALYZE=1 npm run build && open stats.html",
    "start": "serve -s dist -l 3000 -n",
    "start:demo": "serve -s build -l 3000 -n",
    "docs": "node docs/api-reference-generator.js",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "cp package.json package.json.bak && node ./.config/prepublish.js",
    "postpublish": "mv package.json.bak package.json"
  },
  "dependencies": {
    "@dyteinternals/mediasoup-client": "^3.6.89",
    "@dyteinternals/utils": "^2.1.0",
    "@faker-js/faker": "^8.4.1",
    "axios": "^0.25.0",
    "bowser": "^2.11.0",
    "events": "^3.3.0",
    "lodash-es": "^4.17.21",
    "socket.io-client": "^4.7.5",
    "uuid": "^8.3.2",
    "worker-timers": "7.0.60"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/events": "^3.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/long": "4.0.0",
    "@types/node": "^16.0.0",
    "@types/sdp-transform": "^2.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitest/coverage-c8": "^0.28.3",
    "@vitest/coverage-istanbul": "^0.31.1",
    "commitizen": "^4.2.4",
    "concurrently": "^8.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "0.17.12",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^7.0.0",
    "is-ci": "^3.0.0",
    "jsdoc-to-markdown": "^7.1.1",
    "jsdom": "^22.0.0",
    "npm-install-peers": "^1.2.2",
    "rollup": "3.17.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "semantic-release": "^20.0.2",
    "serve": "^12.0.0",
    "ts-node": "^10.0.0",
    "tsup": "^6.7.0",
    "type-fest": "^2.12.1",
    "typescript": "^4.8.3",
    "vite": "4.1.2",
    "vite-plugin-istanbul": "^4.0.0",
    "vitest": "^0.31.3",
    "vitest-github-action": "^1.3.15"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": "since 2016",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
